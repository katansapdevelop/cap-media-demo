{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds watch",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "watch",
        "--with-mocks",
        "--in-memory?"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node-terminal",
      "name": "Run CSV Generator",
      "request": "launch",
      "command": "npm run csv-gen",
      "cwd": "${workspaceFolder}"
    }
  ]
}